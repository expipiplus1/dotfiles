:set -XDataKinds
:set -XPolyKinds
:set -XQuasiQuotes
:set -XFlexibleContexts
:set -XFlexibleInstances
:set -XMultiParamTypeClasses
:set -XScopedTypeVariables
:set -XTypeOperators
:set -XTypeFamilies
:set -XBinaryLiterals
:set -XDeriveDataTypeable
