[user]
  email = git@monoid.al
  name = Joe Hermaszewski
[push]
  default = simple
[alias]
  po = "!git push --set-upstream origin \"$(git rev-parse --abbrev-ref HEAD)\""
  s = status -s
  co = checkout
  cob = checkout -b
  pr = pull-request
  latest = "!git log --all --oneline | head -n1 | cut -f1 -d' '"
  cpl = "!git cherry-pick $(git latest)"
  pf = push --force-with-lease
  authors = "!f(){ set -o pipefail; git blame $1 --line-porcelain | grep 'author ' | grep -v 'Not Committed Yet' | sed 's/author //' | sort | uniq -c | sort -n ; }; f"
  author = "!f(){ set -o pipefail; git blame $1 --line-porcelain | grep 'author ' | grep -v 'Not Committed Yet' | sed 's/author //' | sort | uniq -c | sort -nr | head -n1 | sed 's/ *[0-9]* *//' ; }; f"
  cane = commit --amend --no-edit
[core]
  editor = vim
[merge]
  tool = vimdiff
[mergetool]
  keepBackup = false
[github]
	user = expipiplus1
[color]
	ui = auto
[rebase]
	instructionFormat = [%an] %s
[oh-my-zsh]
  only-branch = 1

